{
    "builds": [
        {
            "src": "pacman_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb", "runtime": "python 3.14.0"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "pacman_project/wsgi.py"
        }
    ]
}